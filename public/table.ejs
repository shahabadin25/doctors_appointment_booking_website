<!DOCTYPE html>
<html>
<head>
  <title>Patients</title>
  <style>
    table {
      border-collapse: collapse;
      width: 100%;
    }
    th, td {
      padding: 8px;
      text-align: left;
      border-bottom: 1px solid #ddd;
    }
  </style>
</head>
<body>
  <h1>Patients</h1>
  <table>
    <thead>
      <tr>
        <th>Index</th>
        <th>Name</th>
        <th>Age</th>
        <th>Gender</th>
        <th>Blood Group</th>
        <th>Phone Number</th>
        <th>Medical History</th>
        <th>Date</th>
      </tr>
    </thead>
    <tbody>
      <% userDetails.forEach((user,index) => { %>
        <tr>
          <td><%= index + 1 %></td>
          <td><%= user.name %></td>
          <td><%= user.age %></td>
          <td><%= user.gender %></td>
          <td><%= user.blood_group %></td>
          <td><%= user.phone_number %></td>
          <td><%= user.medical_history %></td>
          <td><%= user.date.toDateString() %></td>
        </tr>
        <% }); %>
    </tbody>
  </table>
</body>
</html>